<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RapActivity"
            android:label="RAP LIST"/>
        <activity android:name=".PopActivity"
            android:label="POP LIST"/>
        <activity android:name=".RockActivity"
            android:label="ROCK LIST"/>
        <activity android:name=".ElectronicActivity"
            android:label="ELECTRONIC LIST"/>
        <activity android:name=".NowPlayingActivity"
            android:label="NOW PLAYING"/>
    </application>

</manifest>